[submodule "nvim/.config/nvim/sneaps"]
	path = nvim/.config/nvim/sneaps
	url = https://github.com/rafamadriz/friendly-snippets
	shallow = true

[submodule "zsh/.config/zsh/.oh-my-zsh"]
	path = zsh/.config/zsh/.oh-my-zsh
	url = https://github.com/ohmyzsh/ohmyzsh.git

[submodule "zsh/.config/zsh/oh-my-zsh-custom/plugins/bol"]
	path = zsh/.config/zsh/oh-my-zsh-custom/plugins/bol
	url = https://github.com/ikhurramraza/bol
	ignore = dirty

